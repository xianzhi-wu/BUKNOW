	  background-image: -moz-linear-gradient(bototm,#e0e0e0,#e0e0e0 50%,transparent 50%), -moz-linear-gradient(top,#e0e0e0,#e0e0e0 50%,transparent 50%);
	  background-image: -o-linear-gradient(bototm,#e0e0e0,#e0e0e0 50%,transparent 50%), -o-linear-gradient(top,#e0e0e0,#e0e0e0 50%,transparent 50%);
