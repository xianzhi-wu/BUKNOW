/** CSS Document
* Dec: global style
* Author: Zhi;
* Date: 2015-11-30;
**/
html, body, div, dl, dt, dd, ul, ol, li, h1, h2, h3, h4, h5, h6, form, input, button, textarea, p, th, td, a {margin:0;padding:0;border:0;}
h1, h2, h3, h4, h5, h6 {font-weight:normal;}
body {min-width:320px;max-width:640px;margin:0 auto;font-family:微软雅黑, "Microsoft Yahei";background:#f2f2f2;-webkit-font-smoothing:antialiased;color:#333;}
i {font-style:normal;display:block;}
a {cursor:pointer;text-decoration:none;color:#333;display:block;}
a:active, a:hover {outline:none;}
ul, li {list-style:none;}
img {width:100%; display:block;border:0;}
button, input, select, textarea {font-size:100%;outline:0;border-radius:0;border:none;}
input:-webkit-autofill {background-color:transparent !important;}
textarea, input, a {-webkit-tap-highlight-color:rgba(0, 0, 0, 0);}
textarea, input {resize:none;}
.clearfix:before, .clearfix:after {content:"";display:table;}
.clearfix:after {clear:both;}

.single-line {overflow:hidden;text-overflow:ellipsis;white-space:nowrap;}
h3 {font-size:15px;}
h4 {font-size:14px;}
/*弹性布局*/
.box-layout {display:-webkit-box;display:-webkit-flex;display:-ms-flexbox;display:flex;}
/*.box-layout {display:-webkit-box;display:-moz-box;width:100%;box-sizing:border-box;}*/
/*垂直居中*/
.center-v {-webkit-box-align:center;-webkit-align-items:center;-ms-flex-align:center;align-items:center;}
.items-stretch {-webkit-box-align:stretch; -webkit-align-items:stretch; -ms-flex-align:stretch; align-items:stretch;}
/*水平居中*/
.center-h {-webkit-box-pack:center;-ms-flex-pack:center;-webkit-justify-content:center;justify-content:center;}
.box-col {-webkit-box-flex:1;-webkit-flex:1;-ms-flex:1;flex:1;}
/*.box-col {-webkit-box-flex:1;-moz-box-flex:1;display:block;}*/
/*弹窗透明背景*/
.overlay {position:fixed;z-index:9800;top:0px;bottom:0;left:0px;right:0;background:rgba(0, 0, 0, 0.4);cursor:pointer;}
.popup-tip {position:fixed;z-index:9990;top:50%;margin:-20px auto 0;left:0;right:0;color:#fff;border-radius:4px;height:36px;line-height:36px;border-radius:6px;text-align:center;background:rgba(0, 0, 0, 0.7)!important;filter:alpha(opacity=80);background:#000;font-size:14px;width:200px;}
.hide {display:none;}

.index-header {position:fixed;z-index:9900;right:0;left:0;top:0;margin:0 auto;max-width:640px;height:50px;line-height:50px;background:-webkit-linear-gradient(top,rgba(0,0,0,.8),rgba(0,0,0,0));background:linear-gradient(top,rgba(0,0,0,.8),rgba(0,0,0,0));-moz-transition:background 0.3s linear;-o-transition:background 0.3s linear;transition:background 0.3s linear;}
.purple-bg{background:rgba(67,13,73, 1)}
.index-header .city {padding:0 8px;color:#fff;font-size:15px;}
.index-header .search-trigger {padding-left:38px;height:32px;line-height:32px;border-radius:16px;background:#fff;margin:9px 5px 0;color:#999;font-size:14px;position:relative;}
.index-header i {height:20px;width:20px;background:url(../images/icons/bn-icons-mix.png) no-repeat;background-size:220px auto;}
.index-header .search-icon {background-position:0 0;position:absolute;top:5px;left:12px;}
.index-header .menu-icon {background-position:-20px 0;margin:15px auto;}
.menu-dropdown {position:absolute;top:50px;left:0;right:0px;background:#fff;overflow:hidden;height:0;transition:height 0.2s linear;-webkit-transition:height 0.2s linear;-moz-transition:height 0.2s linear;}
.menu-dropdown a{height:40px;line-height:40px;border-bottom:1px solid #e0e0e0;text-align:center;font-size:14px;}

.header {height:44px;line-height:44px;background:#fafafa;text-align:center;border-bottom:1px solid #e0e0e0;box-sizing:border-box;position:relative;z-index:9200;}
.header .back {position:relative;height:100%;padding-left:44px;color:#fff;}
.header .back:after {border:1px solid #430d49;position:absolute;left:13px;top:50%;margin-top:-7px;display:block;content:"";width:12px;height:12px;border-top:none;border-right:none;-webkit-transform:rotate(45deg);transform:rotate(45deg);}
.header .header-title {color:#430d49;font-size:16px;font-weight:600;}
.header-icon-btn {height:100%;width:44px;overflow:hidden;}
.blank-space {width:44px;}

/*首页焦点图*/
.banner_wrapper {position:relative; width:100%; padding-top:40%; overflow:hidden;}
.banner_wrapper .sliders, .banner_wrapper .holder {position:absolute; top:0; left:0; width:100%; height:100%; overflow:hidden;}
.pointers { position:absolute; bottom:0; width:100%;text-align:center; height:20px; line-height:20px;}
.pointers i:first-child {margin-left:0;}
.pointers i {display:inline-block; margin-left:4px; width:8px; height:8px; background:#fff; border-radius:4px; opacity:0.8;}
.pointers i.active {background:#430d49; box-sizing:border-box;}
/*首页分类*/
.index-cate-wrapper {background:#fff;padding-bottom:12px;overflow:hidden;border-bottom:1px solid #e0e0e0;}
.index-cate-wrapper a {width:20%;float:left;text-align:center;padding-top:12px;}
.index-cate-wrapper a i {width:30px;height:30px;background:url(../images/icons/bn-icons-category.png) no-repeat;background-size:150px auto;margin:0 auto;}
.index-cate-wrapper a span {display:block;font-size:12px;margin-top:4px;}
.index-cate-wrapper a .cate-new {background-position:0 0;}
.index-cate-wrapper a .cate-malls {background-position:-30px 0;}
.index-cate-wrapper a .cate-food {background-position:-60px 0;}
.index-cate-wrapper a .cate-lady {background-position:-90px 0;}
.index-cate-wrapper a .cate-shopping {background-position:-120px 0;}
/*大牌驾到*/
.brands-wrapper {position:relative;background:#fff;margin:12px 0;}
.brands-wrapper:after {position:absolute;content:'';bottom:0;right:0;left:0;height:1px;border-bottom:1px solid #e0e0e0;}
.brands-list {position:relative;overflow:hidden;}
.brands-list ul {width:726px;background:#fff;overflow:hidden;}
.brands-list ul li {float:left;width:120px;border-right:1px solid #efefef;}
.brands-list ul li a .brand-img {width:120px;height:120px;padding:10px;background:#fff;box-sizing:border-box;overflow:hidden;}

/*搜索页面*/
.search-tip {background:#fff;border-radius:4px;border:1px solid #f4f4f4;text-align:left;font-size:14px;padding:0 12px;color:#666;height:32px;line-height:32px;margin-top:6px;}
.search-container {background:#fff;padding:16px 2%;border-bottom:1px solid #e0e0e0;font-size:14px;}
.search-input-cont {border:1px solid #e6e6e6;border-right:none;border-radius:4px 0 0 4px;}
.search-input {background:#fafafa;height:24px;line-height:24px;width:96%;padding:8px 2%;border-radius:4px 0 0 4px;}
.search-btn {height:42px;width:68px;line-height:42px;font-size:15px;text-align:center;color:#fff;background:#259fc4;border-radius:0 4px 4px 0;}
.hot-words, .search-history {background:#fff;line-height:40px;margin-bottom:16px;font-size:14px;}
.hot-words ul li {border-bottom:1px solid #e0e0e0;overflow:hidden;}
.hot-words ul li a{float:left;position:relative;width:25%;height:40px;text-align:center;border-radius:2px;color:#333;}
.hot-words ul li a:not(:nth-child(4n)):after {position:absolute;content:"";right:0;top:50%;height:16px;margin-top:-8px;border-left:#e6e6e6;}
.hot-words ul li:last-child {border-bottom:none;}
.search-history ul li{position:relative;border-bottom:1px solid #e0e0e0;line-height:20px;}
.search-history ul li a {padding:10px;}
.clear-history {height:36px;line-height:36px;font-size:12px;text-align:center;}
/*搜索结果*/
.searchResult-category {background:#fff;border-bottom:1px solid #e0e0e0;height:44px;line-height:44px;font-size:14px;text-align:center;}
.searchResult-category a {position:relative;color:#333;padding:0 4px;}
.searchResult-category a:after {display:block;position:absolute;content:'';right:0;top:10px;height:24px;border-right:1px solid #e6e6e6;}
.searchResult-category a:last-child:after{display:none;}
.searchResult-category a.active{color:#259fc4;}
/*登录注册页面*/
.login-wrapper {min-height:100%;background:url(../images/user/login-bg.jpg) no-repeat;background-size:100% auto;overflow:hidden;}
.login-wrapper .back {position:relative;left:8px;height:44px;line-height:44px;width:30px;padding-left:14px;color:#fff;font-size:14px;}
.login-wrapper .back:after {border:1px solid #fff;position:absolute;left:4px;top:50%;margin-top:-7px;display:block;content:"";width:10px;height:10px;border-top:none;border-right:none;-webkit-transform:rotate(45deg);transform:rotate(45deg);}
.default-img {height:70px;width:70px;margin:30px auto 40px;overflow:hidden;}
.login-form {margin:0 16px 20px;}
.login-form input {width:100%;height:24px;line-height:24px;padding:10px 0;background:none;font-size:14px;}
.login-form .input-line {border-bottom:1px solid #ccc;padding-left:40px;height:44px;position:relative;margin-top:10px;}
.login-form .v {padding-right:130px;}
.login-form .input-line i {width:20px;height:21px;position:absolute;top:11px;left:10px;background:url(../images/icons/bn-icons-user.png) no-repeat;background-size:80px auto;}
.login-form .input-line .person-icon {background-position:0 -69px;}
.login-form .input-line .password-icon {background-position:-28px -69px;}
.login-form .input-line .vericode-icon {background-position:-55px -69px;}
.login-form .sub-line {margin:6px;overflow:hidden;}
.login-form .sub-line a {height:44px;line-height:44px;font-size:12px;}
.login-form .sub-line .forget-password {float:left;color:#666;}
.login-form .sub-line .signup {float:right;color:#db384c;}
.login-form .login-btn {height:44px;line-height:44px;text-align:center;border:1px solid #ccc;border-radius:3px;margin-top:10px;}
.login-form .login-btn#reset {margin-top:24px;}
.login-form .input-line .get-code {position:absolute;top:4px;right:5px;height:32px;line-height:32px;text-align:center;border-radius:3px;padding:0 12px;border:1px solid #ccc;font-size:14px;color:#999;}
.login-form .agree-line {height:32px;line-height:32px;margin:0 8px;font-size:14px;color:#666;margin-top:8px;}
.login-form .agree-line a {display:inline-block;color:#f00;}
.login-wrapper .login-tips {color:#f00;font-size:14px;text-align:center;visibility:hidden;height:20px;line-height:20px;}

.coupon-icon {background:url(../images/icons/bn-icons-mix.png) no-repeat -38px -32px;background-size:220px auto;height:16px;width:22px;margin-right:4px;display:inline-block;position:relative;top:3px;}
.sale-icon {background:url(../images/icons/bn-icons-mix.png) no-repeat -68px -32px;background-size:220px auto;height:16px;width:19px;margin-right:4px;display:inline-block;position:relative;top:3px;}
/*券或活动列表*/
.deals-group {margin-top:12px;}
.deals-list {margin-bottom:12px;background:#fff;}
.deals-list a {padding:8px;border-bottom:1px solid #e0e0e0;position:relative;font-size:15px;}
.deals-list .brand-name span{color:#666;font-size:12px;}
.deals-list a .img {height:80px;width:90px;border-radius:2px;overflow:hidden;}
.deals-list a .cont {position:relative;height:80px;margin-left:12px;}
.deals-list a .cont .title {margin-top:2px;height:36px;line-height:18px;text-overflow:ellipsis;display:-webkit-box;-webkit-line-clamp:2;-webkit-box-orient:vertical;overflow:hidden;}
.deals-list a .cont .sub-title {color:#db384c;margin-top:4px;font-size:14px;}
.deals-list a .cont .shop-name, .deals-list a .cont .expired-time {position:absolute;left:0;color:#666;font-size:12px;}
.deals-list a.more {padding:0;}
.more {color:#259fc4;font-size:14px;height:44px;line-height:44px;text-align:center;border-bottom:1px solid #e0e0e0;}
/*下拉分类*/
.catelist-nav {position:relative;z-index:9980;background:#fff;}
.catelist-nav .cate {height:44px;line-height:44px;border-bottom:1px solid #e0e0e0;box-sizing:border-box;}
.catelist-nav .cate ul li {font-size:14px;text-align:center;position:relative;}
.catelist-nav .cate ul li.selected a {color:#430d49;}
.catelist-nav .cate ul li a .drop{border:1px solid #666;border-top:none;border-right:none;display:inline-block;width:8px;height:8px;margin-left:8px;-webkit-transform:rotate(-45deg);transform:rotate(-45deg);position:relative;top:-2px;-webkit-transition:all 0.2s linear;transition:all 0.2s linear;-webkit-backface-visibility:hidden;}
.catelist-nav .cate ul li.selected a .drop {-webkit-transform:rotate(135deg);transform:rotate(135deg);top:3px;border:1px solid #430d49;border-top:none;border-right:none;}
.catelist-nav .cate ul li:after {position:absolute;content:'';border-left:1px solid #efefef;height:18px;top:13px;left:0;}
.catelist-nav .cate ul li:first-child:after {border-left:none;}
.dropdown-selector {position:absolute;top:44px;width:100%;height:auto;overflow:hidden;}
.dropdown-selector .selector-wrapper {background:#fafafa;}
.selector-wrapper .selector-view {position:relative;overflow:hidden;}
.selector-wrapper .selector-sider {background:#f2f2f2;position:relative;overflow:hidden;}
.selector-wrapper .selector-view ul li.selected {background:#f2f2f2;}
.selector-wrapper .selector-view ul li.selected a, .selector-wrapper .selector-sider ul li a.selected {color:#430d49;}
.selector-wrapper .selector-view ul li.selected a .goto {border:1px solid #430d49;border-bottom:none;border-left:none;}
.selector-wrapper .selector-view ul li a, .selector-wrapper .selector-sider ul li a {padding:15px;font-size:14px;border-bottom:1px solid #e0e0e0;box-sizing:border-box;position:relative;}
/*最新优惠页面、首页每日推荐*/
.header .filter-icon {background:url(../images/icons/bn-icons-mix.png) no-repeat 0 -32px;background-size:220px auto;height:22px;width:32px;margin:12px auto 0;}
.new-deals, .daily-rec {padding-bottom:2%;}
.new-deals ul li, .daily-rec ul li{width:47%;margin:2% 0 0 2%;float:left;background:#fff;border-radius:4px;padding-bottom:6px;overflow:hidden;}
.img-box {background:#f6f6f6;}
.new-deals ul li .title, .daily-rec ul li .title{font-size:14px;margin:5px 8px;height:18px;line-height:18px;}
.new-deals ul li .sub-title, .daily-rec ul li .sub-title {font-size:12px;color:#999;margin:0 8px;height:14px;line-height:14px;}
.daily-rec {background:#fff;border-bottom:1px solid #e0e0e0;}
.daily-rec ul li {border:1px solid #f4f4f4;box-sizing:border-box;}

.new-cate-wrapper {position:absolute;z-index:9200;top:50px;right:0;left:0;background:#fff;padding-bottom:2%;text-align:center;font-size:14px;}
.new-cate {overflow:hidden;}
.new-cate a{width:22.5%;margin:2% 0 0 2%;border:1px solid #dfdfdf;height:44px;line-height:44px;border-radius:16px;float:left;box-sizing:border-box;}
.new-cate a.on {position:relative;}
.new-cate a.on::after {content:"";background:url(../images/icons/bn-icons-mix.png) no-repeat -95px -32px;background-size:220px auto;height:19px;width:18px;position:absolute;top:0;right:0;}
.new-cate-wrapper .confirm-btn{box-sizing:border-box;color:#db384c;border:1px solid #db384c;border-radius:12px;width:22.5%;margin:2% 2% 0 0;height:40px;line-height:40px;float:right;}

/*全部分类*/
.all-cate-wrapper {margin:8px;}
.all-cate-wrapper h4 {position:relative;padding:6px 0;font-weight:600;}
.all-cate-wrapper h4:after {position:absolute;content:'';top:16px;left:80px;right:0;border-bottom:1px solid #e0e0e0;}
.all-cate-wrapper ul {border:1px solid #e0e0e0;border-radius:4px;background:#fff;overflow:hidden;margin-bottom:6px;}
.all-cate-wrapper ul li {width:25%;float:left;height:44px;line-height:44px;text-align:center;font-size:14px;border-bottom:1px solid #e0e0e0;border-right:1px solid #e0e0e0;box-sizing:border-box;margin-bottom:-1px;}
.all-cate-wrapper ul li:nth-child(4n) {border-right:0;}
.all-cate-wrapper h4 i {background:url(../images/icons/bn-icons-category.png) no-repeat;background-size:150px auto;width:14px;height:14px;margin-right:4px;display:inline-block;position:relative;top:2px;}
.all-cate-wrapper h4 .cate-food-s {background-position:0 -34px;}
.all-cate-wrapper h4 .cate-lady-s {background-position:-16px -34px;}
.all-cate-wrapper h4 .cate-shopping-s {background-position:-33px -34px;}
.all-cate-wrapper h4 .cate-service-s {background-position:-49px -34px;}
.all-cate-wrapper h4 .cate-health-s {background-position:-65px -34px;}
.all-cate-wrapper h4 .cate-travel-s {background-position:-80px -34px;}
.all-cate-wrapper h4 .cate-hotel-s {background-position:-97px -34px;}

/*综合商场*/
.malls-wrapper {margin:12px 12px 0;}
.malls-wrapper ul li {border-radius:4px;overflow:hidden;padding-bottom:26px;position:relative;margin-bottom:12px;}
.malls-wrapper ul li .img {min-height:80px;background:#f2f2f2;}
.malls-wrapper ul li .mall-info {position:absolute;bottom:0;left:0;right:0;}
.malls-wrapper ul li .mall-name {height:52px;width:80px;overflow:hidden;background:#f2f2f2;}
.malls-wrapper ul li .mall-info .info1 ,.malls-wrapper ul li .mall-info .info2 {height:26px;line-height:26px;font-size:14px;padding:0 8px;}
.malls-wrapper ul li .mall-info .info1 {background:rgba(0,0,0,.7);color:#fff;}
.malls-wrapper ul li .mall-info .info2 {background:#fff;padding-right:50px;position:relative;}
.malls-wrapper ul li .mall-info .new {position:absolute;right:5px;bottom:0;font-size:12px;color:#666;}
.malls-wrapper ul li .mall-info .icon-new {background:url(../images/icons/bn-icons-mix.png) no-repeat -120px -32px;width:18px;height:14px;background-size:220px auto;display:inline-block;margin-left:4px;}

/*详情页面*/
.detail-cont, .shop-wrap {background:#fff;}
.detail-cont .detail-img, .shop-wrap .shop-img {background:#f2f2f2;}
.detail-cont .basic-info .title {font-size:15px;height:18px;line-height:18px;overflow:hidden;}
.detail-cont .basic-info .expired-time {color:#666;font-size:12px;margin-top:4px;}

.desc-cont, .comment-cont, .shop-cont {margin:12px 0;background:#fff;}
.desc-cont .brief-info {padding:8px 12px;font-size:14px;line-height:18px;border-bottom:1px solid #e0e0e0;}
.comment-link {position:absolute;top:0;bottom:0;right:0;width:44px;}
.comment {border-bottom:1px solid #e0e0e0;padding:8px 12px;}
.comment .user-img {height:40px;width:40px;border-radius:50%;background:#f2f2f2;overflow:hidden;margin-right:8px;}
.comment .user-name {font-size:14px;}
.comment .user-info {position:relative;height:16px;margin-top:5px;overflow:hidden;}
.comment .comment-time {font-size:12px;color:#666;position:absolute;right:0;top:0;}
.comment .comment-content {font-size:14px;line-height:18px;margin-top:10px;}
.comment-cont.mt {margin-top:0;}
.header-icon-btn .comment-icon {margin-top:11px;}

.detail-cont .basic-info, .shop-cont .shop-info, .shop-wrap .shop-info {padding:8px 0 8px 12px;border-bottom:1px solid #e0e0e0;border-top:1px solid #e0e0e0;}
.detail-cont .basic-info .collect-area, .shop-cont .shop-info .tel, .shop-wrap .shop-info .tel {width:60px;border-left:1px solid #e0e0e0;}
.desc-cont .more-desc, .comment-cont .more-comment, .shop-cont .more-shops, .shop-wrap .more-info {padding:0 12px;height:36px;line-height:36px;color:#6b0480;position:relative;font-size:14px;}

.shop-wrap .more-info span {color:#666;font-size:12px;}
.deal-tip {color:#db384c;font-size:12px;padding:6px 12px;}
.deal-tip i {display:inline-block;margin:0 3px;}

.shop-cont .shop-info h4 , .shop-wrap .shop-info h4{font-size:14px;height:16px;line-height:16px;padding:4px 0;oveflow:hidden;}
.shop-cont .shop-info .address, .shop-wrap .shop-info .address {height:14px;line-height:14px;color:#666;font-size:12px;margin-top:2px;}
.shop-wrap .shop-info {padding-left:40px;position:relative;}
.shop-wrap .shop-info .location-icon {position:absolute;top:18px;left:12px;}
/*图文详情*/
.details {padding:8px;}
.details p, .details img {margin-bottom:8px;}
/*详情页面图标*/
.user-icon {background:url(../images/icons/bn-icons-mix.png) no-repeat -129px -60px;background-size:220px auto;height:21px;width:20px;margin:15px auto 0;}
.share-icon {background:url(../images/icons/bn-icons-mix.png) no-repeat 0 -60px;background-size:220px auto;width:22px;height:22px;margin:14px auto 0;}
.collect-icon {background:url(../images/icons/bn-icons-mix.png) no-repeat -27px -60px;background-size:220px auto;width:20px;height:17px;margin:12px auto 0;}
.header .collect-icon {margin:19px auto 0;}
.collected {background-position:-50px -60px;}
.comment-icon {background:url(../images/icons/bn-icons-mix.png) no-repeat -75px -60px;background-size:220px auto;width:21px;height:21px;margin:6px auto 0;}
.tel-icon {background:url(../images/icons/bn-icons-mix.png) no-repeat -100px -60px;background-size:220px auto;width:24px;height:24px;margin:10px auto 0;}
.location-icon {background:url(../images/icons/bn-icons-mix.png) no-repeat -153px -60px;background-size:220px auto;height:23px;width:18px;}

/*评分*/
.stars {position:relative;top:2px;width:67px;height:12px;background:url(../images/icons/stars.png);background-size:67px auto;}
.star5 {background-position:0 0;}
.star4 {background-position:0 -15px;}
.star3 {background-position:0 -30px;}
.star2 {background-position:0 -44px;}
.star1 {background-position:0 -59px;}

.comment-stars {padding:0 0 8px 0;}
.comment-stars span {display:inline-block;margin-right:4px;color:#333;height:40px;line-height:40px;}
.comment-stars a {height:36px;width:36px;display:inline-block;vertical-align:middle;background:url(../images/icons/icon-star.png);background-size:36px auto;}
.comment-stars a.star-on {background-position:0 -36px;}
.comment-tarea {border:1px solid #e3e3e3;height:auto;border-radius:4px;margin:12px 0;box-sizing:border-box;background:#f6f6f6;}
.comment-tarea textarea {background:#f6f6f6;padding:10px;width:100%;border-radius:4px;box-sizing:border-box;}
.comment-btn {height:40px;line-height:40px;text-align:center;border-radius:4px;background:#259fc4;color:#fff;margin-top:20px;font-size:15px;}
.comment-tip {top:0;margin-top:124px;display:none;}
/*分享*/
.share-wrapper {position:fixed;z-index:9999;left:12px;right:12px;bottom:-208px;max-width:640px;margin:0 auto;-webkit-transition:all 0.3s ease-in; transition:all 0.3s ease-in; -webkit-backface-visibility:hidden;}
.share-wrapper a {height:44px;line-height:44px;font-size:15px;color:#259fc4;background:#fff;text-align:center;box-sizing:border-box;cursor:pointer;}
.share-wrapper a.share-btn {border-bottom:1px solid #e4e4e4;}
.share-wrapper a.share-btn:first-child {border-top-left-radius:4px;border-top-right-radius:4px;}
.share-wrapper a.share-btn:last-child {border-bottom-left-radius:4px;border-bottom-right-radius:4px;border-bottom:none;}
.share-wrapper a.cancel-share {margin:16px 0;border-radius:4px;}

/*分店列表*/
.branch-count {height:32px;line-height:32px;color:#259fc4;margin:0;padding-left:12px;}
.branches-wrapper ul {background:#fff;border-top:1px solid #e0e0e0;}
.branches-wrapper ul li {border-bottom:1px solid #e0e0e0;position:relative;padding:8px 0px;}
.branches-wrapper ul li a {padding-left:12px;}
.branches-wrapper ul li .name {font-size:15px;margin-bottom:4px;color:#000;}
.branches-wrapper ul li .address {color:#666;font-size:14px;padding:0;}
.branches-wrapper ul li .distance {margin-top:2px;font-size:12px;color:#666;}
.branches-wrapper ul li .tel {width:64px;border-left:1px solid #e0e0e0;position:relative;}
.branches-wrapper ul li .tel .tel-icon {margin:0;position:absolute;top:50%;margin:-12px;left:50%;margin-left:-12px;}

/*显示地图*/
.map-container {height:320px;width:100%;background:#f4f4f4;}
.map-location {width:25px;height:32px;background:url(../images/icons/bn-icons-mix.png) no-repeat;background-position:0 -112px;background-size:220px auto;display:block;}
.marker {position:absolute;left:50%;margin-left:-88px;background:#fff;border-radius:4px;bottom:46px;padding:8px;width:160px;text-align:center;opacity:0.9;color:#333;-webkit-filter:drop-shadow(0 0 4px rgba(0,0,0,0.5));font-size:14px;}
.marker:after {content:"";position:absolute;left:50%;margin-left:-8px;bottom:-8px;width:16px;height:16px;-webkit-transform:rotateZ(45deg);-ms-transform:rotateZ(45deg);background:#fff;}
.map-site {position:relative;padding:16px 104px 16px 10px;background:#fff;color:#333;font-size:14px;line-height:18px;border-bottom:1px solid #e0e0e0;}
.go-there {position:absolute;top:50%;margin-top:-16px;right:10px;background:#259fc4;width:84px;height:34px;line-height:34px;text-align:center;border-radius:4px;color:#fff;}

/*电子券商家*/
.ecoupon-wrapper {margin-bottom:1%;overflow:hidden;}
.ecoupon-wrapper ul li {width:32%;border:1px solid #f0f0f0;box-sizing:border-box;float:left;background:#fff;margin:1% 0 0 1%;}
.ecoupon-wrapper ul li a {padding:15px 8px;}
.ecoupon-wrapper ul li a .brand-logo {width:60px;height:60px;margin:0 auto;overflow:hidden;}
.ecoupon-wrapper ul li a .brand-name {height:16px;line-height:16px;font-size:14px;color:#666;margin-top:2px;text-align:center;}
/*电子优惠券列表*/
.ecoupon-list {margin:0 12px;overflow:hidden;}
.ecoupon-list ul li {background-color:#fff;border-radius:3px;overflow:hidden;margin:12px 0;font-size:14px;text-align:center;padding-bottom:4px;}
.ecoupon-list ul li .top {border-bottom:1px dashed #e0e0e0;padding:10px 8px;}
.ecoupon-list ul li .ecoupon-name {color:#db384c;margin-bottom:4px;}
.ecoupon-list ul li .expired-time {margin-top:4px;}
.ecoupon-list ul li .ecoupon-pic {max-height:456px;overflow:hidden;margin-top:1px;position:relative;}
.pic-viewer {position:absolute;z-index:9990;top:0;left:0;height:100%;width:100%;cursor:pointer;}
.pic-viewer img {display:inline-block;vertical-align:middle;}
.hide-img{position:absolute;z-index:9999;bottom:12px;height:32px;line-height:32px;width:84px;color:#fff;left:0;right:0;margin:0 auto;background:rgba(36,159,196,0.7);text-align:center;font-size:15px;border-radius:22px;}
.lock {overflow:hidden;}

/*个人中心*/
.ucenter-top {position:relative;}
.user-box {position:absolute;top:50%;left:50%;margin-left:-100px;margin-top:-53px;width:200px;height:106px;}
.user-box .user-img {height:80px;width:80px;border-radius:50%;overflow:hidden;margin:0 auto;}
.user-box .user-name {height:18px;line-height:18px;margin-top:8px;font-size:14px;color:#fff;text-align:center;}
.user-nav {background:#fff;border-bottom:1px solid #e0e0e0;}
.user-nav a {border-right:1px solid #e0e0e0;text-align:center;font-size:14px;padding:10px 0;}
.user-nav a:last-child {border-right:none;}
.user-nav a span {height:16px;margin-top:2px;display:block;}
.user-nav a i {background:url(../images/icons/bn-icons-user.png) no-repeat;background-size:80px auto;height:22px;width:23px;margin:0 auto;}
.user-nav a .my-comment {background-position:0 0;}
.user-nav a .my-collection {background-position:-27px 0;}
.user-nav a .my-baoliao {background-position:-55px 0;}
.user-nav-line {margin:12px 0;border-top:1px solid #e0e0e0;background:#fff;}
.user-nav-line a {height:44px;line-height:44px;border-bottom:1px solid #e0e0e0;position:relative;padding:0 12px;}
.user-nav-line a i {height:20px;width:20px;background:url(../images/icons/bn-icons-user.png) no-repeat;background-size:80px auto;display:inline-block;position:relative;top:4px;margin-right:4px;}
.user-nav-line a .my-wallet {background-position:0 -25px;}
.user-nav-line a .my-money {float:right;font-size:12px;color:#666;}
.user-nav-line a .my-money span {color:#f00;font-size:14px;}
.user-nav-line a .bind-mobile, .login-form .input-line .mobile {background-position:-28px -25px}
.user-nav-line a .bind-alipay {background-position:-55px -25px}
.user-nav-line a .my-settings {background-position:0 -48px;}
.user-nav-line a .about-us {background-position:-28px -48px;}
.user-nav-line a .newest {background-position:-55px -48px;}
/*我的评论*/
.comment-wrapper ul li {padding:0 0 8px 12px;background:#fff;border-bottom:1px solid #e0e0e0;border-top:1px solid #e0e0e0;margin:10px 0;position:relative;}
.comment-wrapper ul li .comm-title {font-size:14px;color:#666;padding:10px 0;border-bottom:1px solid #e0e0e0;position:relative;margin-bottom:4px;}
.comment-wrapper ul li .comm-time {font-size:12px;color:#999;margin:4px 12px 0 0;text-align:right;}
.comment-wrapper ul li .comm-content {font-size:15px;padding:4px 0;}
/*我的收藏*/
.edit {font-size:12px;color:#666;}
.mycol-nav {background:#fff;border-bottom:1px solid #e0e0e0;box-sizing:border-box;height:44px;line-height:44px;text-align:center;font-size:14px;}
.mycol-nav ul li {position:relative;}
.mycol-nav ul li:after{position:absolute;content:'';height:20px;top:12px;right:0;border-right:1px solid #efefef;}
.mycol-nav ul li:last-child:after {border-right:none;}
.mycol-nav ul li a.active {color:#259fc4;}
.mycol-nav ul li a.active:after {position:absolute;content:'';bottom:0;left:0;right:0;border-bottom:1px solid #259fc4;}
.collection .deals-list {margin:12px 0;}
.deals-list a.pl {padding-left:60px;}
.delete-area {position:absolute;top:26px;left:8px;height:44px;width:44px;}
.delete-area .delete-icon {position:relative;height:24px;width:24px;border-radius:50%;margin:10px auto 0;overflow:hidden;background:#f00;transform:rotate(45deg);
-ms-transform:rotate(45deg);-moz-transform:rotate(45deg);-webkit-transform:rotate(45deg);}
.delete-area .delete-icon:before {position:absolute;content:'';border-left:2px solid #fff;height:16px;left:11px;top:4px;}
.delete-area .delete-icon:after {position:absolute;content:'';border-bottom:2px solid #fff;width:16px;left:4px;top:11px;}
/*商家列表*/
.shops-list {margin-bottom:12px;background:#fff;}
.shops-list ul li {padding:8px 8px 8px 12px;color:#666;font-size:14px;border-bottom:1px solid #e0e0e0;position:relative;}
.shops-list ul li i {margin-right:4px;}
.shops-list ul li .box-col {position:relative;}
.shops-list ul li .shop-name {margin-top:-2px;font-size:15px;color:#333;height:24px;line-height:24px;}
.shops-list ul li .shop-img {height:90px;width:100px;border-radius:4px;overflow:hidden;margin-right:8px;}
.shops-list ul li .shop-tel {display:inline-block;padding-top:6px;height:36px;line-height:36px;}
.shops-list ul li .shop-address {position:absolute;bottom:2px;left:0;height:24px;line-height:24px;}
.shops-list ul  .pl {padding-left:60px;}
.shop-icon, .tel-icon-grey, .location-icon-grey {background:url(../images/icons/bn-icons-mix.png) no-repeat;background-size:220px auto;height:20px;width:20px;display:inline-block;position:relative;top:4px;}
.shop-icon {background-position:-40px -85px;}
.tel-icon-grey {background-position:-20px -85px;}
.location-icon-grey {background-position:0 -85px;}
.collection .shops-list {margin-top:12px;}
/*我的设置*/
.settings-wrapper ul {margin-bottom:12px;border-radius:2px;background:#fff;}
.settings-wrapper ul li {border-bottom:1px solid #e0e0e0;padding-left:12px;height:44px;line-height:44px;}
.settings-wrapper ul li a {position:relative;padding:0 30px 0 64px;height:100%;font-size:12px;color:#666;text-align:right;}
.settings-wrapper ul li a span {position:absolute;left:0;top:0;font-size:14px;color:#333;}
.quit-btn {border-radius:2px;background:#259fc4;height:44px;line-height:44px;text-align:center;font-size:15px;color:#fff;}
/*关于我们*/
.about-wrapper {font-size:14px;}
.about-wrapper .p {background:#fff;margin-bottom:12px;padding:12px;}
.about-wrapper .p .logo {width:78px;height:74px;margin:12px auto 32px;}
.about-wrapper .p h3 {font-size:16px;font-weight:600;margin-bottom:3px;}
.about-wrapper .p p {line-height:20px;}
/*爆料*/
.post-wrapper {background:#fff;margin-bottom:12px;}

.goto {position:absolute;right:12px;top:50%;margin-top:-4px;border:1px solid rgba(0,0,0,0.2);width:8px;height:8px;border-bottom:none;border-left:none;-webkit-transform:rotate(45deg);transform:rotate(45deg);}
.tip{position:fixed;left:50%;margin-left:-120px;top:50%;margin-top:-24px;color:#fff;border-radius:6px;height:48px;line-height:48px;width:240px;text-align:center;background:rgba(0, 0, 0, 0.8)!important;filter:alpha(opacity=80);background:#000;}
/*加载动画*/
.loading {margin:12px auto;position:relative;border:1px solid #259fc4;border-left:none;-webkit-animation:loader 0.5s infinite linear;animation:loader 0.5s infinite linear;}
.loading, .loading:after {border-radius:50%;width:16px;height:16px;}
@-webkit-keyframes loader { 0% {-webkit-transform:rotate(0deg);transform:rotate(0deg);} 100% {-webkit-transform: rotate(360deg);transform: rotate(360deg);} }
@keyframes loader { 0% {-webkit-transform: rotate(0deg);transform: rotate(0deg);} 100% {-webkit-transform: rotate(360deg);transform: rotate(360deg);} }
/*h3*/
.brands-wrapper h3, .deals-list h3, .desc-cont h3, .comment-cont h3, .shop-cont h3, .daily-rec h3, .deals-list .brand-name {padding:6px 12px;border-bottom:1px solid #e0e0e0;position:relative;}
.comment-main, .settings-wrapper {margin:12px;}

.no-result {margin:24px 0;font-size:15px;text-align:center;}

.footer {text-align:center;margin-top:12px;padding:16px 0 50px;background:#fff;border-top:1px solid #e0e0e0;}
.footer ul li {display:inline-block;position:relative;}
.footer ul li a {color:#666;height:32px;line-height:32px;font-size:14px;padding:0 12px;}
.footer ul li:not(:last-child):after {position:absolute;content:'';height:14px;top:9px;right:0;border-right:1px solid #999;}
.footer .copyright {color:#d1d1d1;margin-top:20px;text-shadow:0 1px 1px #fff;font-size:12px;}

